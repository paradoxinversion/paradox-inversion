{
  "name": "paradox-inversion",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "keystone": "^4.0.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "multer": "0.1.8",
    "pug": "^2.0.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.6.5",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.18.9",
    "parcel-bundler": "^1.11.0"
  },
  "engines": {
    "node": "9.3.0"
  },
  "scripts": {
    "start": "NODE_ENV='production' NODE_PATH=.  node ./dist/server/server.js",
    "start-dev": "concurrently \"yarn run watch-server\" \"yarn run watch-client\" ",
    "build-client": "parcel build ./app/index.html --log-level 5 --out-dir dist/client",
    "watch-client": "parcel ./app/index.html --no-cache --open --log-level 3",
    "build-server": "NODE_ENV='production' parcel build ./server/server.js --target node --no-minify --no-source-maps --out-dir dist/server",
    "watch-server": "nodemon server/server.js",
    "build": "rimraf dist && yarn run build-server && yarn run build-client"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
